<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    app:startDestination="@id/fragment_repositories">

    <fragment
        android:id="@+id/fragment_repositories"
        android:name="io.syslogic.github.fragment.RepositoriesFragment"
        tools:layout="@layout/fragment_repositories"
        android:label="@string/app_name">

        <action
            android:id="@+id/action_RepositoriesFragment_to_RepositoryFragment"
            app:destination="@id/fragment_repository"/>

    </fragment>

    <fragment
        android:id="@+id/fragment_repository"
        android:name="io.syslogic.github.fragment.RepositoryFragment"
        tools:layout="@layout/fragment_repository"
        android:label="fragment_repository">
        <argument
            android:name="itemId"
            app:argType="long"/>
    </fragment>

</navigation>
